{
  "name": "my-next-app",
  "version": "1.0.0",
  "description": "A Next.js application with a custom Express backend for API and frontend integration.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev:backend": "nodemon server.js",
    "dev:frontend": "next dev -p 4000",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "lint": "next lint",
    "format": "prettier --write .",
    "build": "next build",
    "start:prod": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.34",
    "autoprefixer": "^10.4.15",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "next": "^13.4.0",
    "oracledb": "^6.7.0",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.13",
    "tailwindcss": "^3.4.15"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "concurrently": "^9.1.0",
    "eslint": "9.15.0",
    "eslint-config-next": "15.0.3",
    "husky": "^8.0.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.0.0",
    "typescript": "^5.6.3"
  },
  "author": "Sory Ibrahima Berete Berthesory@gmail.com",
  "license": "ISC"
}
